    "from os import walk\n",
    "import pandas as pd\n",
    "\n",
    "data_file_paths = next(walk('./data'), (None, None, []))[2]\n",
    "\n",
    "frames = []\n",
    "\n",
    "for data in data_file_paths:\n",
    "    frames.append(pd.read_csv('./data/' + data))\n",
    "    \n",
    "data_df = pd.concat(frames, join='outer', ignore_index=True)\n",
    "    \n",
    "print(data_df)"
   ]
